// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Cell {
  id           Int       @id @default(autoincrement())
  operator     String
  area         String
  city         String
  cb           String
  lac          Int?
  cid          Int?
  sectors_gsm  Int[]
  sectors_dcs  Int[]
  lac_3g       Int?
  cid_3g       Int?
  sectors_3g   Int[]
  lac_u900     Int?
  cid_u900     Int?
  sectors_u900 Int[]
  date         DateTime? @db.Date
  address      String
  remark       String
  latitude     Float?
  longitude    Float?
}
